_zimfw_mv() {
  if command cmp -s ${2} ${1}; then
    _zimfw_print -PR "<%= okay %>%B${2}:%b Already up to date"
  else
    if [[ -e ${2} ]]; then
      command mv -f ${2}{,.old} || return 1
    fi
    command mv -f ${1} ${2} && \
        _zimfw_print -PR "<%= okay %>%B${2}:%b Updated. Restart your terminal for changes to take effect."
  fi
}
